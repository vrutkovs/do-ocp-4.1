{
  "ignition": {
    "config": {
      "merge": [
        {
          "source": "${ignition_url_final}"
        }
      ]
    },
    "security": {
      "tls": {
        "certificateAuthorities": [
          {
            "source": "data:text/plain;charset=utf-8;base64,"
          }
        ]
      }
    },
    "timeouts": {},
    "version": "3.0.0"
  },
  "passwd": {},
  "storage": {
    "files": [
      {
        "group": {},
        "overwrite": true,
        "path": "/etc/hostname",
        "user": {
          "name": "root"
        },
        "contents": {
          "source": "data:text/plain;charset=utf-8;base64,${etc_hostname_encoded}"
        },
        "mode": 420
      },
      {
        "group": {},
        "overwrite": true,
        "path": "/etc/crio/crio.conf",
        "user": {
          "name": "root"
        },
        "contents": {
          "source": "data:text/plain;charset=utf-8;base64,${crioconf_conf_encoded}"
        },
        "mode": 420
      }
    ]
  },
  "systemd": {}
}